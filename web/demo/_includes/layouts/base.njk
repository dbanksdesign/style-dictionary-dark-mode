    <header>
      <nav>
      
      {%- for entry in collections.all | eleventyNavigation %}
        <a class="nav-item{% if entry.url == page.url %} nav-item-active{% endif %}" href="{{ entry.url | url }}">
          <svg width="18" height="18"><use href="#{{ entry.title }}" xlink:href="#{{ entry.title }}"/></svg>
          {{ entry.title }}
        </a>
      {%- endfor %}
      </nav>
    </header>

    <main{% if templateClass %} class="{{ templateClass }}"{% endif %}>

      {{ content | safe }}
    </main>

    <footer></footer>

    <!-- Current page: {{ page.url | url }} -->
    {# <script>
      (function() {
        window.__onThemeChange = function() {
          console.log()
        };
        function setTheme(newTheme) {
          window.__theme = newTheme;
          preferredTheme = newTheme;
          document.body.className = newTheme;
          window.__onThemeChange(newTheme);
        }

        var preferredTheme;
        try {
          preferredTheme = localStorage.getItem('theme');
        } catch (err) { }

        window.__setPreferredTheme = function(newTheme) {
          setTheme(newTheme);
          try {
            localStorage.setItem('theme', newTheme);
          } catch (err) {}
        }

        var darkQuery = window.matchMedia('(prefers-color-scheme: dark)');
        darkQuery.addListener(function(e) {
          console.log(e);
          window.__setPreferredTheme(e.matches ? 'dark' : 'light')
        });

        setTheme(preferredTheme || (darkQuery.matches ? 'dark' : 'light'));
        var colorModeMap = {
          light: '🌙',
          dark: '☀️'
        }
        var modeButton = document.getElementById("color-mode-toggle");
        modeButton.textContent = colorModeMap[preferredTheme];
        
        modeButton.addEventListener('click', event => {
          var newTheme = preferredTheme === 'dark' ? 'light' : 'dark';
          modeButton.textContent = colorModeMap[newTheme];
          window.__setPreferredTheme(newTheme);
        });
      })();
    </script> #}
